{
  "name": "@foxglove/mcap-conformance",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -b tsconfig.json tsconfig.cjs.json",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "generate-inputs": "ts-node --files --project tsconfig.cjs.json scripts/generate-inputs",
    "run-tests": "tsc -b tsconfig.json tsconfig.cjs.json && ts-node --files --project tsconfig.cjs.json scripts/run-tests"
  },
  "devDependencies": {
    "@foxglove/crc": "^0.0.3",
    "@foxglove/eslint-plugin": "1.0.1",
    "@foxglove/tsconfig": "1.1.0",
    "@mcap/core": "workspace:*",
    "@mcap/support": "workspace:*",
    "@types/diff": "^5.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "colors": "1.4.0",
    "commander": "11.1.0",
    "diff": "^5.1.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.0.1",
    "js-yaml": "4.1.0",
    "json-stable-stringify": "1.0.2",
    "kaitai-struct": "0.10.0",
    "kaitai-struct-compiler": "0.10.0",
    "lodash": "4.17.21",
    "prettier": "3.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "5.2.2"
  }
}
